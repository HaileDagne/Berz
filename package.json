{
  "name": "foodmenu",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "vite --base=/",
    "build": "vite build",
    "build:prod": "cross-env NODE_ENV=production vite build",
    "build:hostinger": "cross-env NODE_ENV=production vite build && echo 'Build complete! Upload the dist folder contents to your Hostinger public_html directory.'",
    "deploy": "npm run build && gh-pages -d dist",
    "predeploy": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "serve": "vite preview --port 4173"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.1.1",
    "globals": "^16.3.0",
    "rimraf": "^6.0.1",
    "terser": "^5.44.0",
    "vite": "^7.1.0"
  }
}
